{
  "version": 2,
  "projects": {
    "product-feedback": {
      "root": "apps/product-feedback",
      "sourceRoot": "apps/product-feedback",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/product-feedback",
            "outputPath": "dist/apps/product-feedback"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "product-feedback:build",
            "dev": true,
            "proxyConfig": "apps/product-feedback/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "product-feedback:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "product-feedback:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/product-feedback"],
          "options": {
            "jestConfig": "apps/product-feedback/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/product-feedback/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "product-feedback-api": {
      "root": "apps/product-feedback-api",
      "sourceRoot": "apps/product-feedback-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/product-feedback-api",
            "main": "apps/product-feedback-api/src/main.ts",
            "tsConfig": "apps/product-feedback-api/tsconfig.app.json",
            "assets": ["apps/product-feedback-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/product-feedback-api/src/environments/environment.ts",
                  "with": "apps/product-feedback-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "product-feedback-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/product-feedback-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/product-feedback-api"],
          "options": {
            "jestConfig": "apps/product-feedback-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "product-feedback-e2e": {
      "root": "apps/product-feedback-e2e",
      "sourceRoot": "apps/product-feedback-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/product-feedback-e2e/cypress.json",
            "tsConfig": "apps/product-feedback-e2e/tsconfig.e2e.json",
            "devServerTarget": "product-feedback:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "product-feedback:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/product-feedback-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "product-feedback-ui": {
      "root": "libs/product-feedback/ui",
      "sourceRoot": "libs/product-feedback/ui/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/product-feedback/ui",
            "tsConfig": "libs/product-feedback/ui/tsconfig.lib.json",
            "project": "libs/product-feedback/ui/package.json",
            "entryFile": "libs/product-feedback/ui/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/product-feedback/ui/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/product-feedback/ui/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/product-feedback/ui"],
          "options": {
            "jestConfig": "libs/product-feedback/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/product-feedback/ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/product-feedback-ui",
            "config": {
              "configFolder": "libs/product-feedback/ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "product-feedback-ui-e2e": {
      "root": "apps/product-feedback-ui-e2e",
      "sourceRoot": "apps/product-feedback-ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/product-feedback-ui-e2e/cypress.json",
            "tsConfig": "apps/product-feedback-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "product-feedback-ui:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "product-feedback-ui:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/product-feedback-ui-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-util-styles": {
      "root": "libs/shared/util/styles",
      "sourceRoot": "libs/shared/util/styles/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/shared/util/styles",
            "main": "libs/shared/util/styles/src/index.ts",
            "tsConfig": "libs/shared/util/styles/tsconfig.lib.json",
            "assets": ["libs/shared/util/styles/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/util/styles/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/util/styles"],
          "options": {
            "jestConfig": "libs/shared/util/styles/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "product-feedback"
}
